<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luis.tipsdesalud">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.luis.tipsdesalud.ActividadListaArticulos"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.luis.tipsdesalud.ActividadDetalleArticulo"
            android:label="@string/titulo_actividad_detalle"
            android:parentActivityName="com.luis.tipsdesalud.ActividadListaArticulos"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.luis.tipsdesalud.ActividadListaArticulos" />
        </activity>
    </application>

</manifest>
